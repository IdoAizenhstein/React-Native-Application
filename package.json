  "private": true,
  "scripts": {
    "android": "react-native run-android --variant=devDebug --appIdSuffix=dev",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "rm -rf node_modules/@react-native-community/cameraroll-ios/android",
    "lint": "eslint -c .eslintrc './src/**/*.js'",
    "lint:fix": "eslint --fix -c .eslintrc './src/**/*.js'",
    "postinstallOld": "cat config/RCTImageLoader.mm > node_modules/react-native/Libraries/Image/RCTImageLoader.mm && cat config/react_native_pods.rb > node_modules/react-native/scripts/react_native_pods.rb",
    "podinstall": "npx pod-install",
    "create:android:apk:dev": "cd android/ && ./gradlew assembleDevRelease && cd ..",
    "create:android:apk:prod": "cd android/ && ./gradlew assembleProdRelease && cd ..",
    "create:android:apk:prod:bundle": "cd android/ && ./gradlew bundleRelease && cd ..",
    "clean": "react-native-clean-project",
    "reinstall-pods": "cd ios && rm -rf Pods/ && pod install && cd ..",
    "clean-droid-build": "cd android && ./gradlew app:clean && cd .."
  },
